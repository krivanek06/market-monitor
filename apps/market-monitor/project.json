{
  "name": "market-monitor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/market-monitor/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/market-monitor/browser",
        "index": "apps/market-monitor/src/index.html",
        "main": "apps/market-monitor/src/main.ts",
        "polyfills": ["apps/market-monitor/src/polyfills.ts"],
        "tsConfig": "apps/market-monitor/tsconfig.app.json",
        "assets": ["apps/market-monitor/src/favicon.ico", "apps/market-monitor/src/assets"],
        "styles": ["@angular/material/prebuilt-themes/indigo-pink.css", "apps/market-monitor/src/styles.scss"],
        "scripts": [],
        "allowedCommonJsDependencies": [
          "utils/",
          "assert/",
          "https-browserify",
          "stream-http",
          "querystring-es3",
          "stream-browserify",
          "url/",
          "crypto-browserify",
          "browserify-zlib",
          "path-browserify",
          "os-browserify",
          "highcharts",
          "highcharts/modules/no-data-to-display"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "market-monitor:build:production"
        },
        "development": {
          "browserTarget": "market-monitor:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "market-monitor:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/market-monitor/**/*.ts", "apps/market-monitor/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/market-monitor/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "market-monitor:build"
      }
    },
    "server": {
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/market-monitor/server",
        "main": "apps/market-monitor/src/main.server.ts",
        "tsConfig": "apps/market-monitor/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@nguniversal/builders:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "market-monitor:build:development",
          "serverTarget": "market-monitor:server:development"
        },
        "production": {
          "browserTarget": "market-monitor:build:production",
          "serverTarget": "market-monitor:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {
        "version": 2,
        "browserTarget": "market-monitor:build:production",
        "serverTarget": "market-monitor:server:production"
      }
    }
  }
}
