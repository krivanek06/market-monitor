  "scripts": {
    "mm:serve": "nx run market-monitor:serve",
    "mm:dev:ssr": "nx run market-monitor:serve-ssr",
    "mm:serve:ssr": "node dist/apps/market-monitor/server/main.js",
    "mm:build:ssr": "nx build market-monitor && nx build market-monitor:server",
    "mm:build-sentry:ssr": "nx build market-monitor && nx build market-monitor:server && yarn run sentry:sourcemaps",
    "mm:deploy:ssr": "nx build market-monitor && firebase target:apply hosting market-monitor market-monitor-prod && firebase deploy --only hosting:market-monitor",
    "mm:deploy-sentry:ssr": "nx build market-monitor && yarn run sentry:sourcemaps && firebase target:apply hosting market-monitor market-monitor-prod && firebase deploy --only hosting:market-monitor",
    "mm:cloud-function:serve": "nx build market-monitor-cloud-functions && firebase emulators:start --only functions",
    "mm:cloud-functions:kill": "kill -9 $(lsof -ti:4000,4001)",
    "mm:cloud-function:debug": "nx build market-monitor-cloud-functions && firebase emulators:start --only functions --inspect-functions",
    "mm:cloud-function:deploy": "firebase deploy --only functions",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org eduard-krivanek --project javascript-angular apps/market-monitor && sentry-cli sourcemaps upload --org eduard-krivanek --project javascript-angular apps/market-monitor"
  },
