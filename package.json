  "scripts": {
    "mm:serve": "nx run market-monitor:serve",
    "mmd:serve": "nx run dashboard:serve",
    "mm:dev:ssr": "nx run market-monitor:serve-ssr",
    "mm:serve:ssr": "node dist/apps/market-monitor/server/main.js",
    "mm:prerender:ssr": "nx prerender market-monitor:server --configuration=production",
    "mm:build:ssr": "nx build market-monitor --configuration=production && nx build market-monitor:server --configuration=production",
    "mm:build-sentry:ssr": "yarn mm:build:ssr && yarn run sentry:sourcemaps",
    "mm:deploy:ssr": "nx build market-monitor && firebase target:apply hosting market-monitor market-monitor-prod && firebase deploy --only hosting:market-monitor-prod",
    "mm:deploy-sentry:ssr": "nx build market-monitor && yarn run sentry:sourcemaps && firebase target:apply hosting market-monitor market-monitor-prod && firebase deploy --only hosting:market-monitor-prod",
    "mm:cloud-function:build": "nx build cloud-functions --prod && node cp-angular",
    "mm:cloud-function:serve": "yarn mm:cloud-function:build && firebase emulators:start --only functions",
    "mm:cloud-function:kill": "kill -9 $(lsof -ti:4000,4001,9299)",
    "mm:cloud-function:debug": "yarn mm:cloud-function:build && firebase emulators:start --only functions --inspect-functions",
    "mm:cloud-function:deploy": "yarn mm:cloud-function:build && firebase deploy --only functions",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org eduard-krivanek --project javascript-angular apps/market-monitor && sentry-cli sourcemaps upload --org eduard-krivanek --project javascript-angular apps/market-monitor"
  },
