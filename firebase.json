{
  "hosting": [
    {
      "target": "market-monitor",
      "source": ".",
      "frameworksBackend": {
        "region": "europe-west1"
      }
    }
  ],
  "functions": [
    {
      "source": "dist/apps/market-monitor-cloud-functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": ["yarn nx run market-monitor-cloud-functions:build"]
    }
  ]
}
