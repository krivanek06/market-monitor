{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch chrome market-monitor client",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}/market-monitor"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Debug Node",
        "port": 9229,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
